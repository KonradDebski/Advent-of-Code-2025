{
  "name": "ts-project-template",
  "version": "1.0.2",
  "description": "Vite bundler with TypeScript & SASS compile|autoprefix|minimize",
  "main": "index.html",
  "type": "module",
  "author": "DrewnioQ",
  "scripts": {
    "dev": "vite serve --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "postbuild": "postcss dist/assets/*.css -u autoprefixer -r --no-map"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "modern-normalize": "^3.0.1",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.7.4",
    "sass": "^1.94.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.6"
  }
}
